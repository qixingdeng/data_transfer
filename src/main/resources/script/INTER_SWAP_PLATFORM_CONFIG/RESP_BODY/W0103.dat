<?xml version="1.0" encoding="utf-8"?>
<xsl:stylesheet version="2.0"
    xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:fn="http://www.w3.org/2005/xpath-functions" xmlns:xalan="http://xml.apache.org/xalan">
    <xsl:output method="xml" version="1.0" encoding="utf-8" indent="no" />
    <xsl:strip-space elements="*" />
    <xsl:template match="/">
        <xsl:variable name="rows"  select="//smParamNode[@id='BdVehicleLineGnssLog']/smParamNode[not(@*)]" />
        <json>
             <code>0</code>
             <data>
            <xsl:for-each select="$rows">
                   <gnss>
                    <accesscode><xsl:value-of select=".//smParamNode[@key='accesscode']/@value" /></accesscode>
                    <trans_type><xsl:value-of select=".//smParamNode[@key='trans_type']/@value" /></trans_type>
                    <vin><xsl:value-of select=".//smParamNode[@key='vin']/@value" /></vin>
                    <color><xsl:value-of select=".//smParamNode[@key='color']/@value" /></color>
                    <cur_accesscode><xsl:value-of select=".//smParamNode[@key='cur_accesscode']/@value" /></cur_accesscode>
                    <lon><xsl:value-of select=".//smParamNode[@key='lon']/@value" /></lon>
                    <lat><xsl:value-of select=".//smParamNode[@key='lat']/@value" /></lat>
                    <warn><xsl:value-of select=".//smParamNode[@key='warn']/@value" /></warn>
                    <state><xsl:value-of select=".//smParamNode[@key='state']/@value" /></state>
                    <elevation><xsl:value-of select=".//smParamNode[@key='elevation']/@value" /></elevation>
                    <od_rate><xsl:value-of select=".//smParamNode[@key='od_rate']/@value" /></od_rate>
                    <locator_rate><xsl:value-of select=".//smParamNode[@key='locator_rate']/@value" /></locator_rate>
                    <mileage><xsl:value-of select=".//smParamNode[@key='mileage']/@value" /></mileage>
                    <direction><xsl:value-of select=".//smParamNode[@key='direction']/@value" /></direction>
                    <position_time><xsl:value-of select=".//smParamNode[@key='position_time']/@value" /></position_time>
                    <recv_time><xsl:value-of select=".//smParamNode[@key='recv_time']/@value" /></recv_time>
                </gnss>
            </xsl:for-each>
        </data>
            </json>
          
    </xsl:template>
</xsl:stylesheet>