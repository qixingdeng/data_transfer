<?xml version="1.0" encoding="utf-8"?>
<xsl:stylesheet version="2.0"
   xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
   xmlns:xs="http://www.w3.org/2001/XMLSchema"
   xmlns:fn="http://www.w3.org/2005/xpath-functions"
   xmlns:xalan="http://xml.apache.org/xalan">
  <xsl:output method="xml" version="1.0" encoding="gb2312" indent="no"/>
  <xsl:strip-space elements="*"/>
  <xsl:template match="/">
    <xsl:variable name="busiOrdId" select="//busiOrdId[text()]"/>
    <selectParamRoot>
       <column>
        <column>entNameChn</column>
        <column>entNameEn</column>
        <column>economicType</column>
        <column>regiAmount</column>
        <column>regiAddrChn</column>
        <column>contactPhone</column>
        <column>contactFax</column>
        <column>contactEmail</column>
        <column>bizRegNo</column>
        <column>legalPersonName</column>
      </column>
       <wheres>
          <funcName>andBusiOrdIdIsNotNull</funcName>
        </wheres>
      <xsl:if test="$busiOrdId!=''">
        <wheres>
          <funcName>andBusiOrdIdEqualTo</funcName>
          <paramList>
            <xsl:value-of select="$busiOrdId"/>
          </paramList>
        </wheres>
      </xsl:if>
      <mappername>OrdEntInfo</mappername>
      <tablename>ORD_ENT_INFO</tablename>
    </selectParamRoot>
  </xsl:template>
</xsl:stylesheet>