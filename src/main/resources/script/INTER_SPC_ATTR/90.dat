<?xml version="1.0" encoding="utf-8"?>
<xsl:stylesheet version="2.0"
   xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
   xmlns:xs="http://www.w3.org/2001/XMLSchema"
   xmlns:fn="http://www.w3.org/2005/xpath-functions"
   xmlns:xalan="http://xml.apache.org/xalan">
  <xsl:output method="xml" version="1.0" encoding="gb2312" indent="no"/>
  <xsl:strip-space elements="*"/>
  <xsl:template match="/">
    <xsl:param name="economicType" select="economicType"/>
    <selectParamRoot>
       <column>
        <column>typeParamDesc</column>
      </column>
      <xsl:if test="$economicType!=''">
        <wheres>
          <funcName>andTypeParamCodeEqualTo</funcName>
          <paramList>
            <xsl:value-of select="$economicType"/>
          </paramList>
        </wheres>
      </xsl:if>
        <wheres>
          <funcName>andTypeCodeLike</funcName>
          <paramList>ECON_TYPE%</paramList>
        </wheres>
      <mappername>SmParamDetail</mappername>
      <tablename>SM_PARAM_DETAIL</tablename>
    </selectParamRoot>
  </xsl:template>
</xsl:stylesheet>