<?xml version="1.0" encoding="utf-8"?>
<xsl:stylesheet version="2.0"
   xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
   xmlns:xs="http://www.w3.org/2001/XMLSchema"
   xmlns:fn="http://www.w3.org/2005/xpath-functions"
   xmlns:xalan="http://xml.apache.org/xalan">
  <xsl:output method="xml" version="1.0" encoding="gb2312" indent="no"/>
  <xsl:strip-space elements="*"/>
  <xsl:template match="/">
    <xsl:param name="busiOrdId" select="busiOrdId"/>
    <selectParamRoot>
       <column>
        <column>shipMgrType</column>
        <column>shipMgrName</column>
      </column>
      <wheres>
          <funcName>andBusiOrdIdIsNotNull</funcName>
        </wheres>
        <wheres>
          <funcName>andBusiOrdIdEqualTo</funcName>
          <paramList>
            <xsl:value-of select="$busiOrdId"/>
          </paramList>
        </wheres>
      <wheres>
          <funcName>andShipMgrTypeIn</funcName>
          <paramList>1,2</paramList>
        </wheres>
      <mappername>OrdShipManagerInfo</mappername>
      <tablename>ORD_SHIP_MANAGER_INFO</tablename>
    </selectParamRoot>
  </xsl:template>
</xsl:stylesheet>